<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <Button
        android:id="@+id/writeJsioBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:text="writeJsioBtn" />


    <TextView
        android:id="@+id/readJsonText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="readJsonText" />

    <Button
        android:id="@+id/readJsioBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:text="readJsioBtn" />

    <TextView
        android:id="@+id/writeJsonText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="writeJsonText" />

</LinearLayout>